{
    "name": "timgrohman/ps-xqmaileon",
    "description": "Prestashop XQueue-Maileon Module",
    "authors": [
        {
            "name": "Tim Grohmann",
            "email": "hi@timgrohmann.de"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "xqueue/maileon-api-client": "^1.8.0"
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\Module\\XQMaileon\\": "src/"
        },
        "classmap": [
            "src/"
        ],
        "exclude-from-classmap": []
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "type": "prestashop-module",
    "license": "ISC",
    "require-dev": {
        "prestashop/php-dev-tools": "^4.3"
    }
}
